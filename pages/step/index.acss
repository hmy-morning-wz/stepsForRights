  justify-content: center;
}

.prize-name {
  width: 196rpx;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Semibold;
  font-weight: 600;
  color: #333333;
  line-height: 45rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.recieve-btn {
  width: 108rpx;
  height: 48rpx;
  background: linear-gradient(180deg, #ff8902, #fea702 100%);
  border-radius: 24rpx;
  margin-left: 24rpx;
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: center;
